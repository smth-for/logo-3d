  "keywords": [
    "3d"
  ],
  "author": "Michael Musso <mmusso@smth.it>",
  "license": "LICENCE",
  "devDependencies": {
    "@types/three": "^0.140.0",
    "@vxna/optimize-three-webpack-plugin": "^5.1.3",
    "http-server": "^14.1.0",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "three": "^0.140.2"
  }
}
